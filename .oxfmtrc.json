{
  "$schema": "./node_modules/oxfmt/configuration_schema.json",
  "printWidth": 180,
  "tabWidth": 2,
  "useTabs": false,
  "bracketSpacing": true,
  "semi": true,
  "singleQuote": false,
  "trailingComma": "none",
  "endOfLine": "lf",
  "ignorePatterns": ["package-lock.json", "pnpm-lock.yaml", "yarn.lock", "bun.lock", "bun.lockb", "/static/", "**/components/ui"]
}
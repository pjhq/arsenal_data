{
  "name": "arsenal",
  "type": "module",
  "scripts": {
    "arsenal": "bun run sort-data && bun run format && bun run --bun scripts/generate_arsenal.ts",
    "loadouts": "bun run --bun scripts/generate_loadouts.ts",
    "extract": "bun run --bun scripts/extract_config.ts",
    "sort-data": "bun run --bun scripts/sort_data.ts",
    "check": "oxlint --type-aware --type-check && oxfmt --check",
    "lint": "oxlint --type-aware --type-check --fix",
    "format": "oxfmt --write"
  },
  "dependencies": {},
  "devDependencies": {
    "@types/bun": "^1.3.9",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "oxlint-tsgolint": "^0.14.2"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}